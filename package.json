{
  "name": "@polyup/commitlint-config",
  "version": "0.2.1",
  "description": "Shareable commitlint config enforcing the PolyUp commit convention",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Polyup-Open-Source/commitlint-config.git"
  },
  "keywords": [
    "commitlint",
    "commitlint-config",
    "conventional-changelog",
    "lint"
  ],
  "author": "PolyUp, Inc. (https://polyup.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Polyup-Open-Source/commitlint-config/issues"
  },
  "homepage": "https://github.com/Polyup-Open-Source/commitlint-config#readme",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "husky": "^4.3.0"
  }
}
